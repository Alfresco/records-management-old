<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >

<suite name="TestNG RM Sanity Tests" verbose="1" time-out="300000">

    <test name="Sanity">
        <groups>
            <run>
                <include name="legacy"/>
                <include name="preconditionForSanity"/>
                <exclude name="ignored"/>
            </run>
        </groups>
        <packages>
            <package name="org.alfresco.test.integration.legacy.*"/>
        </packages>
    </test>

</suite>